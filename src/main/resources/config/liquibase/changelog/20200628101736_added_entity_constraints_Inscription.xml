<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Inscription.
    -->
    <changeSet id="20200628101736-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="eleve_id"
                                 baseTableName="inscription"
                                 constraintName="fk_inscription_eleve_id"
                                 referencedColumnNames="id"
                                 referencedTableName="eleve"/>

        <addForeignKeyConstraint baseColumnNames="niveau_id"
                                 baseTableName="inscription"
                                 constraintName="fk_inscription_niveau_id"
                                 referencedColumnNames="id"
                                 referencedTableName="niveau"/>

        <addForeignKeyConstraint baseColumnNames="annee_scolaire_id"
                                 baseTableName="inscription"
                                 constraintName="fk_inscription_annee_scolaire_id"
                                 referencedColumnNames="id"
                                 referencedTableName="annee_scolaire"/>

    </changeSet>
</databaseChangeLog>
